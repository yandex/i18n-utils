{
  "name": "postcss-logical-fallback",
  "version": "1.2.0",
  "description": "PostCSS plugin for logical fallback props",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "postcss-logical-fallback"
  ],
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "lint": "eslint .",
    "stylelint": "stylelint **/*.css --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "pre-commit": "concurrently --names \"Lint,Test\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run lint\" \"npm run test\"",
    "prepublishOnly": "npm run build",
    "checkPackageVer": "node --experimental-json-modules scripts/check-package-ver.mjs",
    "ci:linterFailed": "node scripts/ci/send-linter-failed.mjs",
    "ci:testsFailed": "node scripts/ci/send-tests-failed.mjs",
    "ci:published": "node --experimental-json-modules scripts/ci/send-published.mjs",
    "ci:publishFailed": "node --experimental-json-modules scripts/ci/send-publish-failed.mjs",
    "ci:checkVersionFailed": "node scripts/ci/send-check-version-failed.mjs"
  },
  "author": "Alexey Erhskov <a-ershkov@yandex-team.ru>",
  "license": "MIT",
  "repository": "https://github.com/yandex/i18n-utils/packages/postcss-logical-fallback",
  "engines": {
    "node": ">=14.0.0"
  },
  "peerDependencies": {
    "postcss": "^8.4.21"
  },
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@types/stylelint": "14.0.0",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "clean-publish": "^3.4.2",
    "concurrently": "7.6.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-jest": "^25.2.2",
    "jest": "29.5.0",
    "prettier": "2.8.4",
    "rollup": "3.18.0",
    "rollup-plugin-esbuild": "5.0.0",
    "stylelint": "15.5.0",
    "stylelint-prettier": "3.0.0",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "FORCE_COLOR=1 npm run pre-commit"
    }
  },
  "dependencies": {
    "postcss-value-parser": "4.2.0"
  }
}
