{
  "name": "stylelint-use-logical-spec",
  "version": "6.2.2",
  "description": "Enforce usage of logical properties and values in CSS",
  "main": "index.cjs.js",
  "module": "index.es.mjs",
  "files": [
    "index.cjs.js",
    "index.cjs.js.map",
    "index.es.mjs",
    "index.es.mjs.map"
  ],
  "scripts": {
    "build": "rollup -c .rollup.js",
    "test": "stylelint-tape",
    "lint": "eslint *.ts lib/*.ts",
    "lint:fix": "eslint *.ts lib/*.ts --ignore-path .arcignore --fix",
    "pretest": "npm run build",
    "prepublishOnly": "npm run build",
    "checkPackageVer": "node --experimental-json-modules scripts/check-package-ver.mjs"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "postcss-value-parser": "4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@types/node": "18.14.6",
    "@types/stylelint": "13.13.1",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "babel-eslint": "^10.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "prettier": "2.8.1",
    "rollup": "^2.33.1",
    "stylelint-tape": "^2.0.0",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "stylelint": "^13.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "css",
    "logical",
    "properties",
    "property",
    "flow",
    "relative",
    "values",
    "value",
    "ltr",
    "rtl",
    "directional",
    "dir",
    "inline",
    "block",
    "start",
    "end",
    "align",
    "border",
    "clear",
    "float",
    "margin",
    "padding",
    "size",
    "text"
  ]
}
